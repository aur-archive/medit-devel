<?xml version="1.0" ?>
<style-scheme id="womvamp" name="Womwolf" version="1.0">
	<author/>
	<_description>A rip from WombVamp theme (which is a rip from the Wombat theme).</_description>

	<style background="#151515" name="current-line"/>
	<style background="#656565" name="cursor"/>
	<style background="#090909" foreground="#f6f3e8" name="text"/>
	<style background="#111111" foreground="#444444" name="line-numbers"/>
	<style foreground="#99968b" italic="true" name="def:comment"/>
	<style foreground="#e5786d" name="def:constant"/>
	<style foreground="#95e454" italic="true" name="def:string"/>
	<style foreground="#cae682" name="def:identifier"/>
	<style foreground="#cae682" name="def:function"/>
	<style foreground="#cae682" name="def:type"/>
	<style foreground="#8ac6f2" name="def:statement"/>
	<style foreground="#8ac6f2" name="def:keyword"/>
	<style foreground="#e5786d" name="def:preprocessor"/>
	<style foreground="#e5786d" name="def:number"/>
	<style foreground="#e7f6da" name="def:specials"/>

	<!-- Language specific styles -->
	<style foreground="#aaeeaa" name="diff:added-line"/>
	<style foreground="#FFFF7A" name="diff:changed-line"/>
	<style foreground="#DE5959" name="diff:removed-line"/>
	<style name="diff:special-case" use-style="def:constant"/>
	<style name="diff:location" use-style="def:statement"/>
	<style name="diff:diff-file" use-style="def:type"/>

	<!-- Comments -->
	<style name="def:shebang" foreground="#007AF5" bold="true"/>
	<style name="def:doc-comment-element" italic="true"/>
	<style name="def:underlined" italic="true" underline="true"/>

	<!-- Bracket Matching -->
	<style name="bracket-match"               foreground="#9FCC7C" background="#000000" bold="true"/>
	<style name="bracket-mismatch"            foreground="#CC877C" background="#000000" bold="true"/>
</style-scheme>

